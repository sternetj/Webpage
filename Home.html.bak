    <script language="javascript" type="text/javascript">
        function setDimensions() {
            var height = 0;
            var body = window.document.body;
            if (window.innerHeight) {
                height = window.innerHeight;
            } else if (body.parentElement.clientHeight) {
                height = body.parentElement.clientHeight;
            } else if (body && body.clientHeight) {
                height = body.clientHeight;
            }

            var width = getWidth();

            var element = document.getElementById('cover');
            element.style.height = (height + "px");
            var heightFactor = [1, 1, 1, 2, 2, 1, 4];
            var constFator = [0, 0, 0, 0, 0, -4, 0];
            var widthFactor = [-1, -1, -1, -1, -1, 3, 3];
            var sectionIdArray = ['experience', 'about', 'resume', 'ventures', 'progressive', 'experiencesection', 'background'];
            for (var i = 0; i < sectionIdArray.length; i++) {
                element = document.getElementById(sectionIdArray[i]);
                element.style.height = ((heightFactor[i] * height + constFator[i]) + "px");
                if (widthFactor[i] >= 0) {
                    element.style.width = (widthFactor[i] * width + "px");
                }
            }
            //height of image
            element = document.getElementById('resumeImage');
            element.style.height = (0.70 * (height) + "px");
            element.style.padding = (((0.15 * (height)) / 2) + "px 0px " + ((0.15 * (height)) / 2) + "px 0px");
        }
