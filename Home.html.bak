    <script type="text/javascript">
        var bounceCount = 0;
        $("a.verticle").click(function (e) {
            // Prevent a page reload when a link is pressed
            e.preventDefault();
            // Scroll
            $('html, body').animate({ scrollTop: $($(this).attr("href")).offset().top }, 500);
        });
        $("a.left").click(function (e) {
            // Prevent a page reload when a link is pressed
            e.preventDefault();
            // Scroll
            $('html, body').animate({ scrollLeft: $($(this).attr("href")).offset().left }, 500);
        });
        $("a.right").click(function (e) {
            // Prevent a page reload when a link is pressed
            e.preventDefault();
            // Scroll
            $('html, body').animate({ scrollLeft: "+=" + $($(this).attr("href")).offset().left }, 500);
        });
        $('.animated').mouseover(function () {
            if (bounceCount == 0) { $(this).addClass("bounce"); }
            else {
                bounceCount++;
                $(this).one('animationiteration webkitAnimationIteration', function () {
                    $(this).addClass("bounce");
                });
            }
        });
        $('.animated').mouseout(function () {
            $(this).one('animationiteration webkitAnimationIteration', function() {
                $(this).removeClass("bounce");
            });
        });
    </script>
